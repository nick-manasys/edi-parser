form: FIXEDWIDTH
structures:
- id: 'CompanyContacts'
  name: Company contact information
  tagStart: 0
  tagLength: 1
  data:
  - groupId: 'ITEM'
    count: '>1'
    items:
    - id: 'COMPANY'
      tag: '0'
      name: Company Information Record
      values:
      - { name: 'Company Name', type: AN, minLength: 0, maxLength: 20 }
      - { name: 'Industry Classification Code', type: N0, minLength: 0, maxLength: 6 }
      - { name: 'Street Address', type: AN, minLength: 0, maxLength: 20 }
      - { name: 'City', type: AN, minLength: 0, maxLength: 12 }
      - { name: 'State', type: AN, minLength: 0, maxLength: 2 }
      - { name: 'Zip Code', type: N0, minLength: 0, maxLength: 5 }
      - { name: 'Phone Number', type: N0, minLength: 0, maxLength: 10 }
    - id: 'CONTACT'
      tag: '1'
      name: Contact Person Record
      count: '>1'
      values:
      - { name: 'First Name', type: AN, minLength: 0, maxLength: 10 }
      - { name: 'Last Name', type: AN, minLength: 0, maxLength: 15 }
      - { name: 'Title', type: AN, minLength: 0, maxLength: 15 }
      - { name: 'Email Address', type: AN, minLength: 0, maxLength: 20 }
      - { name: 'Street Address', type: AN, minLength: 0, maxLength: 20 }
      - { name: 'City', type: AN, minLength: 0, maxLength: 12 }
      - { name: 'State', type: AN, minLength: 0, maxLength: 2 }
      - { name: 'Zip Code', type: N0, minLength: 0, maxLength: 5 }
      - { name: 'Phone Number', type: N0, minLength: 0, maxLength: 10 }
      - { name: 'Phone Extension', type: N0, minLength: 0, maxLength: 5 }
      - { name: 'Effective Date', type: DT, minLength: 0, maxLength: 8 }
